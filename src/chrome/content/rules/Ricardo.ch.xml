<ruleset name="Ricardo.ch">
        <target host="ricardo.ch" />
        <target host="www.ricardo.ch" />
        <securecookie host="^(?:.*\.)?ricardo\.ch$" name=".*" />

        <exclusion pattern="^http://ricardo\.ch/$"/>
        <rule from="^http://(?:www\.)?ricardo\.ch/" to="https://www.ricardo.ch/"/>
</ruleset>
