<!--
	For other UK government coverage, see GOV.UK.xml.


	NB: Server sends no certificate chain, see https://whatsmychaincert.com

-->
<ruleset name="UK Information Commissioner's Office" default_off="missing certificate chain">

	<!--	Direct rewrites:
				-->
	<target host="ico.gov.uk"/>
	<target host="www.ico.gov.uk"/>


	<rule from="^http:"
		to="https:" />

</ruleset>
