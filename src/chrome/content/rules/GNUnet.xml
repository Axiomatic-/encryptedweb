<ruleset name="GNUnet">
	<target host="gnunet.org" />
	<target host="www.gnunet.org" />
	
	<test url="https://www.gnunet.org/" />

	<securecookie host="^(?:.*\.)?gnunet\.org$" name=".+" />

	<rule from="^http:" to="https:" />
	
	<rule from="^https://www\.gnunet\.org/" to="https://gnunet.org/" />
</ruleset>
